{
  "title": "",
  "description": "",
  "variables": {
    "employeeListSDP": {
      "type": "vb/ServiceDataProvider",
      "defaultValue": {
        "endpoint": "businessObjects/getall_Employee",
        "keyAttributes": "id",
        "itemsPath": "items",
        "responseType": "getallEmployeeResponse"
      }
    },
    "vPageEmployeeDetail": {
      "type": "getallEmployeeResponseObjectType"
    },
    "vPageSearchCriteria": {
      "type": {
        "EmployeeID": "string",
        "Salary": "number",
        "SalaryRadioSetValue": "string"
      }
    }

  },
  "metadata": {},
  "types": {
    "@decorators.getallEmployeeResponse": {
      "endpoint": "getall_Employee",
      "service": "businessObjects",
      "path": "response"
    },
    "@decorators.getallEmployeeResponseObjectType": {
      "endpoint": "getall_Employee",
      "service": "businessObjects",
      "path": "response-_-items[0]"
    },
    "getallEmployeeResponse": {
      "items": [
        {
          "department": "number",
          "email": "string",
          "firstName": "string",
          "id": "number",
          "lastName": "string",
          "lastUpdateDate": "string",
          "lastUpdatedBy": "string",
          "phoneNumber": "string",
          "salary": "number"
        }
      ]
    },
    "getallEmployeeResponseObjectType": {
      "createdBy": "string",
      "creationDate": "string",
      "department": "number",
      "departmentRelation": "number",
      "email": "string",
      "firstName": "string",
      "hireDate": "string",
      "id": "number",
      "lastName": "string",
      "lastUpdateDate": "string",
      "lastUpdatedBy": "string",
      "phoneNumber": "string",
      "salary": "number"
    }
  },
  "eventListeners": {
    "buttonAction": {
      "chains": [
        {
          "chainId": "ButtonActionChainSearchEmployee"
        }
      ]
    },
    "hyperlinkClick": {
      "chains": [
        {
          "chainId": "HyperlinkClickChainMoreDetailEmployee",
          "parameters": {
            "key": "{{ $current.key }}",
            "index": "{{ $current.index }}",
            "current": "{{ $current }}"
          }
        }
      ]
    },
    "toggleDrawer": {
      "chains": [
        {
          "chainId": "application:toggleDrawerHandler"
        }
      ]
    }

  },
  "imports": {
    "components": {
      "oj-button": {
        "path": "ojs/ojbutton"
      },
      "oj-collapsible": {
        "path": "ojs/ojcollapsible"
      },
      "oj-dialog": {
        "path": "ojs/ojdialog"
      },
      "oj-form-layout": {
        "path": "ojs/ojformlayout"
      },
      "oj-input-date": {
        "path": "ojs/ojdatetimepicker"
      },
      "oj-input-text": {
        "path": "ojs/ojinputtext"
      },
      "oj-label": {
        "path": "ojs/ojlabel"
      },
      "oj-label-value": {
        "path": "ojs/ojlabelvalue"
      },
      "oj-option": {
        "path": "ojs/ojoption"
      },
      "oj-progress-bar": {
        "path": "ojs/ojprogress-bar"
      },
      "oj-radioset": {
        "path": "ojs/ojradioset"
      },
      "oj-sample-export-data": {
        "path": "oj-sample/export-data/loader"
      },
      "oj-table": {
        "path": "ojs/ojtable"
      }

    }
  }
}